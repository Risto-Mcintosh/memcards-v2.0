{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off",
    "comma-dangle": "off"
  }
}
